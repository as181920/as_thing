<h1>Note Detail:</h1>
<p>
  <b>Name:</b>
  <%= @as_note.name %>
</p>

<p>
  <b>Comment:</b>
  <%= raw @as_note.comment %>
</p>

<p>
  <b>Public:</b>
  <%= raw @as_note.public %>
</p>

<%= link_to 'Back', as_notes_path %> |
<%= link_to 'Edit', edit_as_note_path(@as_note) %> |
<%= link_to 'Destroy', @as_note, :confirm => 'Are you sure?', :method => :delete %> |
<%= link_to "Labels", as_note_as_labels_path(@as_note) %> |
<%= link_to "data", as_note_as_values_path(@as_note) %>
