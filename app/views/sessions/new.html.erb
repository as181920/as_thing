<div class="header_menu">
  <%= link_to 'Public_Notes', "public" %> &nbsp;
</div>

<div id="login">
  <h1>Log in</h1>

  <%= form_tag sessions_path do %>
    <p>
      <%= label_tag :email %><br />
      <%= text_field_tag :email, params[:email] %>
    </p>
    <p>
      <%= label_tag :password %><br />
      <%= password_field_tag :password %>
    </p>
    <p class="button"><%= submit_tag("Login") %></p>
  <% end %>
</div>

